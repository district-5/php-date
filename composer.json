{
  "name": "district5/date",
  "description": "District5 Date Library",
  "keywords": ["php", "district5", "date"],
  "homepage": "https://github.com/district-5/php-date",
  "license": "mit",
  "authors": [
    {
      "name": "District5",
      "email": "hello@district5.co.uk"
    }
  ],
  "require": {
    "php": ">=8.1",
    "ext-calendar": "*",
    "ext-sockets": "*"
  },
  "autoload": {
    "psr-4": {
      "District5\\Utility\\": "src/Utility/",
      "District5\\Date\\": "src/Date/"
    }
  },
  "autoload-dev": {
    "psr-4": { "District5Tests\\DateTests\\": "tests/DateTests/" }
  },
  "require-dev": {
    "phpunit/phpunit": "9.*"
  }
}
